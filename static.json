{
  "root": "public/",
  "clean_urls": true,
  "https_only": true,
  "error_page": "404.html",
  "redirects": {
    "/loader/": {
      "url": "https://discoverthreejs.com/apps/loader/",
      "status": 301
    },
    "/tutorials/": {
      "url": "https://discoverthreejs.com/book/content/",
      "status": 301
    }
  },
  "headers": {
    "/": {
      "Cache-Control": "no-store, no-cache"
    },
    "**.js": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "cdn-instance.example.com"
    },
    "**.css": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "cdn-instance.example.com"
    },
    "**.json": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "cdn-instance.example.com"
    },
    "**.jpg": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "cdn-instance.example.com"
    },
    "**.jpeg": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "cdn-instance.example.com"
    },
    "**.png": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "cdn-instance.example.com"
    },
    "**.gltf": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "cdn-instance.example.com"
    },
    "**.fbx": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "cdn-instance.example.com"
    },
    "**.ico": {
      "Cache-Control": "public, max-age=31536000",
      "Access-Control-Allow-Origin": "cdn-instance.example.com"
    }
  }
}